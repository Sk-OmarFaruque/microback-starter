{
  "presets": [
    "@ava/stage-4",
    "@ava/transform-test-files"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "root": [
          "./app",
          "./tests"
        ],
        "alias": {
          "~": "./",
          "logic": "./app/logic",
          "model": "./app/models"
        }
      }
    ]
  ]
}